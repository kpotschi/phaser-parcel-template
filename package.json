{
	"name": "phaser-parcel-template",
	"version": "1.0.0",
	"description": "Tamplate running Phaser 3 and Parcel 2",
	"scripts": {
		"start": "parcel src/index.html --open --no-cache",
		"build": "rm -rf ./dist && parcel build src/index.html --dist-dir dist --no-source-maps --public-url ."
	},
	"author": "Kevin Potschien",
	"license": "ISC",
	"dependencies": {
		"phaser": "^3.55.2"
	},
	"devDependencies": {
		"parcel": "^2.4.1",
		"parcel-reporter-static-files-copy": "^1.3.4"
	},
	"staticFiles": {
		"staticPath": "src/assets",
		"staticOutPath": "assets"
	}
}
